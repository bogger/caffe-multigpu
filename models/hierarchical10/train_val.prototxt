# final classification branch
layers {
  name: "cls10_pool"
  type: POOLING
  bottom: "icp9_out"
  top: "cls1_pool"
  pooling_param {
    pool: AVE
    kernel_size: 7
    stride: 1
    pad: 0
    # This padding is somewhat special
  }
}
layers {
  name: "cls10_drop"
  type: DROPOUT
  bottom: "cls10_pool"
  top: "cls10_pool"
  dropout_param {
    dropout_ratio: 0.4
  }
}
layers {
  name: "cls10_fc"
  type: INNER_PRODUCT
  bottom: "cls10_pool"
  top: "cls10_fc"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  inner_product_param {
    num_output: 1000
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layers {
  name: "loss10"
  type: SOFTMAX_LOSS
  bottom: "cls10_fc"
  bottom: "label"
  top: "loss10"
  loss_weight: 1
}
layers {
  name: "accuracy_top1"
  type: ACCURACY
  bottom: "cls10_fc"
  bottom: "label"
  top: "accuracy_top1"
  include: { phase: TEST }
}
layers {
  name: "accuracy_top5"
  type: ACCURACY
  bottom: "cls10_fc"
  bottom: "label"
  top: "accuracy_top5"
  include: { phase: TEST }
  accuracy_param {
    top_k: 5
  }
}
